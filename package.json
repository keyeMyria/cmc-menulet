{
  "name": "cmc-menulet",
  "productName": "CoinMarketCap",
  "author": "Víctor Martínez",
  "version": "0.2.1",
  "description": "CoinMarketCap Menulet",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "yarn build:icon && yarn build:clean && yarn build:webpack",
    "build:clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack",
    "build:icon": "mkdir -p Icon.iconset && sips -z 16 16 Icon.png --out Icon.iconset/icon_16x16.png && sips -z 32 32 Icon.png --out Icon.iconset/icon_16x16@2x.png && sips -z 32 32 Icon.png --out Icon.iconset/icon_32x32.png && sips -z 64 64 Icon.png --out Icon.iconset/icon_32x32@2x.png && sips -z 128 128 Icon.png --out Icon.iconset/icon_128x128.png && sips -z 256 256 Icon.png --out Icon.iconset/icon_128x128@2x.png && sips -z 256 256 Icon.png --out Icon.iconset/icon_256x256.png && sips -z 512 512 Icon.png --out Icon.iconset/icon_256x256@2x.png && sips -z 512 512 Icon.png --out Icon.iconset/icon_512x512.png && iconutil -c icns Icon.iconset && rm -rf Icon.iconset",
    "package": "electron-builder --publish always",
    "release": "cross-env NODE_ENV=production yarn run build && yarn run package",
    "release:all": "cross-env NODE_ENV=production yarn run build && yarn run package:all",
    "production": "cross-env NODE_ENV=production yarn run build && yarn run start",
    "development": "cross-env NODE_ENV=development yarn run start"
  },
  "build": {
    "appId": "net.knoopx.CoinMarketCap",
    "files": [
      "main.js",
      "IconTemplate*",
      "dist/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "mac": {
      "icon": "Icon.icns",
      "target": "dir",
      "category": "public.app-category.finance",
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {
    "electron-default-menu": "^1.0.1",
    "menubar": "^5.2.3"
  },
  "devDependencies": {
    "@vx/group": "^0.0.153",
    "@vx/responsive": "^0.0.158",
    "@vx/scale": "^0.0.153",
    "@vx/shape": "^0.0.160",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-autobind-class-methods": "^5.0.1",
    "babel-plugin-closure-elimination": "^1.3.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.4",
    "cryptocoins-icons": "^2.7.0",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-builder": "^20.10.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.5",
    "mobx": "^4.2.0",
    "mobx-react": "^5.0.0",
    "mobx-state-tree": "^2.0.4",
    "mobx-utils": "^4.0.0",
    "numeral": "^2.0.6",
    "open-color": "^1.6.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.1",
    "react-icons": "^2.2.7",
    "react-motion": "^0.5.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "tachyons": "^4.9.1",
    "tachyons-open-color": "knoopx/tachyons-open-color",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
